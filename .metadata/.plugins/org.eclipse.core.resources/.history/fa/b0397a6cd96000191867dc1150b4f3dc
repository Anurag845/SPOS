import java.util.Scanner;

public class SJF {
	
	public static void min(String[] main) {
		Scanner sc = new Scanner(System.in);
		
		System.out.print("Enter number of processes:- ");
		int n = sc.nextInt();
		
		String Process[] = new String[n];
		for(int i = 0; i < n; i++) {
			System.out.print("Name of process " + i + " :- ");
			Process[i] = sc.next();
			System.out.println("Arrival time of process " + Process[i] + " :- ");
			int arr_time  = sc.nextInt();
			System.out.println("Burst time of process " + Process[i] + " :- ");
		}
		
		
	}
	
}